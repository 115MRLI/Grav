<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="GravView">
    <attr name="colorGenerator" format="string" />
    <attr name="pointGenerator" format="string" />
    <attr name="gravGenerator" format="string" />
    <attr name="animationGenerator" format="string" />
    <attr name="animationGenerators" format="reference" />
    <attr name="grav_color" format="color" />
    <attr name="grav_colors" format="reference" />
    <attr name="cell_size" format="integer" />
    <attr name="variance" format="integer" />
    <attr name="size" format="dimension" />
    <attr name="width" format="dimension" />
    <attr name="height" format="dimension" />
    <attr name="min_animation_time" format="integer" />
    <attr name="max_animation_time" format="integer" />
    <attr name="animation_variance" format="dimension" />
    <attr name="grav_ball_size_anim_from_size" format="dimension" />
    <attr name="grav_ball_size_anim_to_size" format="dimension" />
    <attr name="down_to_up_min_animation_time" format="integer" />
    <attr name="down_to_up_max_animation_time" format="integer" />
    <attr name="shake_direction" format="enum">
      <enum name="horizontal" value="0"/>
      <enum name="vertical" value="1"/>
    </attr>
  </declare-styleable>
</resources>